Cell["Convert ASCII grid to graph", "Subsection",
 CellChangeTimes->{{3.815588747650312*^9, 3.81558874965238*^9}, {
   3.8155888387917023`*^9, 3.815588896476754*^9}, 
   3.8155890047230053`*^9},ExpressionUUID->"e6c0e2c5-8e52-4ec3-aacb-\
17a79aa63a1d"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"importGraph", "[", "inputText_", "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "board", ",", "viewBoard", ",", "doors", ",", "keys", ",", "style", ",",
        "g", ",", "position"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"board", "=", 
       RowBox[{"Flatten", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{"i", ",", "j"}], "}"}], "\[Rule]", 
           RowBox[{"inputText", "\[LeftDoubleBracket]", 
            RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", 
            RowBox[{"Length", "[", "inputText", "]"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", 
            RowBox[{"Length", "[", 
             RowBox[{
             "inputText", "\[LeftDoubleBracket]", "i", 
              "\[RightDoubleBracket]"}], "]"}]}], "}"}]}], "]"}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"viewBoard", "=", 
       RowBox[{"SparseArray", "[", "board", "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"doors", "=", 
       RowBox[{"Sort", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"viewBoard", "\[LeftDoubleBracket]", 
            RowBox[{
             RowBox[{
             "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], ",", 
             RowBox[{
             "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
            "\[RightDoubleBracket]"}], "\[Rule]", "#"}], "&"}], "/@", 
         RowBox[{"Position", "[", 
          RowBox[{"inputText", ",", 
           RowBox[{"_", "?", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"UpperCaseQ", "[", "#", "]"}], "&"}], ")"}]}], ",", 
           RowBox[{"{", "2", "}"}], ",", 
           RowBox[{"Heads", "\[Rule]", "False"}]}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"keys", "=", 
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"\"\<@\>\"", "\[Rule]", "position"}], "}"}], ",", 
         RowBox[{"Sort", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"viewBoard", "\[LeftDoubleBracket]", 
              RowBox[{
               RowBox[{
               "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
               ",", 
               RowBox[{
               "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
              "\[RightDoubleBracket]"}], "\[Rule]", "#"}], "&"}], "/@", 
           RowBox[{"Position", "[", 
            RowBox[{"inputText", ",", 
             RowBox[{"_", "?", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"LowerCaseQ", "[", "#", "]"}], "&"}], ")"}]}], ",", 
             RowBox[{"{", "2", "}"}], ",", 
             RowBox[{"Heads", "\[Rule]", "False"}]}], "]"}]}], "]"}]}], 
        "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"style", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Background", "\[Rule]", 
          RowBox[{"GrayLevel", "[", "0", "]"}]}], ",", 
         RowBox[{"BaseStyle", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{"Directive", "[", 
            RowBox[{"White", ",", 
             RowBox[{"EdgeForm", "[", "]"}], ",", 
             RowBox[{"Opacity", "[", "1", "]"}]}], "]"}], "}"}]}], ",", 
         RowBox[{"VertexShapeFunction", "\[Rule]", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Rectangle", "[", 
             RowBox[{
              RowBox[{"#1", "+", ".5"}], ",", 
              RowBox[{"#1", "-", ".5"}]}], "]"}], "&"}], ")"}]}], ",", 
         RowBox[{"EdgeShapeFunction", "\[Rule]", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Rectangle", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"#1", "[", 
                RowBox[{"[", "1", "]"}], "]"}], "+", ".5"}], ",", 
              RowBox[{
               RowBox[{"#1", "[", 
                RowBox[{"[", "2", "]"}], "]"}], "-", ".5"}]}], "]"}], "&"}], 
           ")"}]}]}], "}"}]}], ";", 
      RowBox[{"g", "=", 
       RowBox[{"Graph", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Union", "[", 
          RowBox[{"Flatten", "[", 
           RowBox[{"Table", "[", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"If", "[", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"StringMatchQ", "[", 
                RowBox[{
                 RowBox[{"viewBoard", "\[LeftDoubleBracket]", 
                  RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], ",", 
                 RowBox[{"Except", "[", 
                  RowBox[{"{", "\"\<#\>\"", "}"}], "]"}]}], "]"}], ",", 
               "\[IndentingNewLine]", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"If", "[", 
                  RowBox[{
                   RowBox[{"StringMatchQ", "[", 
                    RowBox[{
                    RowBox[{"viewBoard", "\[LeftDoubleBracket]", 
                    RowBox[{
                    RowBox[{"i", "+", "1"}], ",", "j"}], 
                    "\[RightDoubleBracket]"}], ",", 
                    RowBox[{"Except", "[", 
                    RowBox[{"{", "\"\<#\>\"", "}"}], "]"}]}], "]"}], ",", 
                   RowBox[{
                    RowBox[{"ToString", "[", 
                    RowBox[{"{", 
                    RowBox[{"i", ",", "j"}], "}"}], "]"}], 
                    "\[UndirectedEdge]", 
                    RowBox[{"ToString", "[", 
                    RowBox[{"{", 
                    RowBox[{
                    RowBox[{"i", "+", "1"}], ",", "j"}], "}"}], "]"}]}]}], 
                  "]"}], ",", "\[IndentingNewLine]", 
                 RowBox[{"If", "[", 
                  RowBox[{
                   RowBox[{"StringMatchQ", "[", 
                    RowBox[{
                    RowBox[{"viewBoard", "\[LeftDoubleBracket]", 
                    RowBox[{"i", ",", 
                    RowBox[{"j", "+", "1"}]}], "\[RightDoubleBracket]"}], ",", 
                    RowBox[{"Except", "[", 
                    RowBox[{"{", "\"\<#\>\"", "}"}], "]"}]}], "]"}], ",", 
                   RowBox[{
                    RowBox[{"ToString", "[", 
                    RowBox[{"{", 
                    RowBox[{"i", ",", "j"}], "}"}], "]"}], 
                    "\[UndirectedEdge]", 
                    RowBox[{"ToString", "[", 
                    RowBox[{"{", 
                    RowBox[{"i", ",", 
                    RowBox[{"j", "+", "1"}]}], "}"}], "]"}]}]}], "]"}]}], 
                "}"}]}], "\[IndentingNewLine]", "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"i", ",", 
               RowBox[{
                RowBox[{"Length", "[", "inputText", "]"}], "-", "1"}]}], 
              "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"j", ",", 
               RowBox[{
                RowBox[{"Length", "[", 
                 RowBox[{
                 "inputText", "\[LeftDoubleBracket]", "i", 
                  "\[RightDoubleBracket]"}], "]"}], "-", "1"}]}], "}"}]}], 
            "]"}], "]"}], "]"}], "\[LeftDoubleBracket]", 
         RowBox[{"2", ";;"}], "\[RightDoubleBracket]"}], "]"}]}], ";"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
